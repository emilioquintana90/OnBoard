<p class="display-1">OnBoard</p>

<%= form_for @kennel do |f| %>
  <div class="form-group">
  <%= f.label :name, class: 'form-text text-muted'%>
  <%= f.text_field :name, class: 'form-text text-muted' %>
  </div>
  
  <div class="form-group">
  <%= f.label :address, class: 'form-text text-muted' %>
  <%= f.text_field :address, class: 'form-text text-muted' %>
  </div>

  <div class="form-group">
  <%= f.label :rate, class: 'form-text text-muted' %>
  <%= f.text_field :rate, class: 'form-text text-muted' %>
  </div>

  <%= f.hidden_field :user_id, value: @user.id%>
  <%= f.hidden_field :capacity, value: @kennel.capacity%>

  <%= f.submit class: 'btn btn-primary btn-lg' %> 
  
<% end %>
