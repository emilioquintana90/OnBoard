<h1 class="display-1"><%= @kennel.name %></h1>

<%= link_to "Back", kennel_owner_user_kennels_path ,class: 'btn btn-info btn-sm'%>
<%= link_to "Edit Kennel Info", edit_kennel_path, class: 'btn btn-outline-success btn-sm'%>

<%= link_to "Delete Kennel", delete_kennel_path,data: {confirm: "Are you sure you want to delete #{@kennel.name}?"}, class: 'btn btn-outline-danger btn-sm'%>

<hr>


<p> Address: <%= @kennel.address %><p>
<p> Rate: $<%= @kennel.rate %> / per night<p>
<p> Capacity: <%= @kennel.capacity %> dens<p>

<h3><%= link_to "View Dens", kennel_dens_path, class: 'btn btn-success btn-lg'%></h3>

